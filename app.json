{
  "expo": {
    "name": "BuyNaBay",
    "slug": "BuyNaBay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/BuyNaBay4.png",
    "scheme": "buynabay",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/BuyNaBay4.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.joevel_berana.BuyNaBay"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/BuyNaBay4.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/BuyNaBay4.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "eas": {
        "projectId": "dba9d734-f9d7-4765-a759-216548d538bd"
      }
    }
  }
}
